.principal
	grid-area: primary
	margin: 20px 0
.perfil-principal
	& p
		font-size 1.5rem
		text-align: left
.subcontainer--principal
	display: flex
.contacto-card
	display flex
	flex-direction: column
	justify-content: space-between
	align-items: center
	width: auto
	height: auto
	& .icon-contacto
